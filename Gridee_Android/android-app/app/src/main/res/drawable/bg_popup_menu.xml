<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <corners android:radius="16dp" />
            <stroke
                android:width="1dp"
                android:color="#E0E0E0" />
            <padding
                android:left="0dp"
                android:top="8dp"
                android:right="0dp"
                android:bottom="8dp" />
        </shape>
    </item>
</layer-list>
